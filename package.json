{
  "name": "openapi-generator-clients",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/BradenM/openapi-generator-clients",
    "directory": "packages/clients"
  },
  "author": "Braden Mars <bradenmars@bradenmars.me>",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "domain": {
      "import": "./dist/domain/v1/index.mjs",
      "types": "./dist/domain/v1/index.mjs"
    },
    "domain/v1": {
      "import": "./dist/domain/v1/index.mjs",
      "types": "./dist/domain/v1/index.mjs"
    },
    "server": {
      "import": "./dist/server/v1/index.mjs",
      "types": "./dist/server/v1/index.mjs"
    },
    "server/v1": {
      "import": "./dist/server/v1/index.mjs",
      "types": "./dist/server/v1/index.mjs"
    },
    "clever/v1": {
      "import": "./dist/clever/v1/index.mjs",
      "types": "./dist/clever/v1/index.mjs"
    },
    "clever": {
      "import": "./dist/clever/v1/index.mjs",
      "types": "./dist/clever/v1/index.mjs"
    }
  },
  "homepage": "https://github.com/BradenM/openapi-generator-clients/tree/main/packages/clients/README.md",
  "bugs": {
    "url": "https://github.com/BradenM/openapi-generator-clients/issues"
  },
  "scripts": {
    "post-process:lint": "pushd ../../ && eslint_d --fix \"**/*.{json,ts?(x)}\" && popd",
    "post-process:format": "prettier --write --ignore-path ../../.prettierignore --config ../../.prettierrc ./**/*",
    "post-process": "nr post-process:lint && nr post-process:format",
    "generate:batch": "openapi-generator-cli batch --clean --root-dir ../viewmodels -v config.yaml",
    "generate": "openapi-generator-cli generate",
    "build:clients": "nr generate -- && nr post-process && nr build",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "dev:build": "pnpm run dev"
  },
  "peerDependencies": {
    "reflect-metadata": "~0.1"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "btoa": "^1.2.1",
    "form-data": "^4.0.0",
    "inversify": "^6.0.1",
    "node-fetch": "^3.2.0",
    "rxjs": "^7.5.4",
    "url-parse": "^1.5.9",
    "whatwg-fetch": "~3.6.2"
  }
}
