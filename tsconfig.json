{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "skipLibCheck": true,
    "noEmit": false,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "suppressExcessPropertyErrors": true
  },
  "include": ["*.ts", "**/*"]
}
